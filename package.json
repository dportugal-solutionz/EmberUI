{
  "name": "ember-ui",
  "version": "0.0.0",
  "scripts": {
    "build send": "npm run build && npm run archive-deploy",
    "watch": "ng build --watch",
    "archive-deploy": "npm run ch5archive  && npm run ch5deploy & npm run upload",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "tw watch":" npx tailwindcss -o src/tailwind.scss -w",
    "ch5archive": "ch5-cli archive -p EmberUI -d dist/EmberUI -o dist/ch5z",
    "ch5deploy": "ch5-cli deploy -t web -H 172.16.48.12 -p  ./dist/ch5z/EmberUI.ch5z",
    "upload": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./UploadTp.ps1"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@crestron/ch5-crcomlib": "^1.2.0",
    "@crestron/ch5-webxpanel": "^1.2.5",
    "rxjs": "~6.6.0",
    "serilogger": "^0.4.1",
    "sub-events": "^1.8.9",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.16",
    "@angular/cli": "~12.2.16",
    "@angular/compiler-cli": "~12.2.0",
    "@types/jasmine": "~3.8.0",
    "@types/moo": "^0.5.5",
    "@types/node": "^12.11.1",
    "autoprefixer": "^10.4.4",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "postcss": "^8.4.12",
    "tailwindcss": "^3.0.23",
    "typescript": "~4.3.5"
  }
